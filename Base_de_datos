# **BASES DE DATOS**
**TABLAS Y DATOS SUGERIDOS**

#### **Tablas**

CREATE DATABASE cross_platform;
USE cross_platform;

###### - 1 Tabla countries.

CREATE TABLE IF NOT EXISTS countries (
  isocode VARCHAR(6) PRIMARY KEY,
  name VARCHAR(50) ,
  alfaisotwo VARCHAR(2) UNIQUE,
  alfaisothree VARCHAR(4) UNIQUE
) ENGINE = INNODB;

###### - 2 Tabla subdivisioncategories.

CREATE TABLE IF NOT EXISTS subdivisioncategories (
  id INT(11) AUTO_INCREMENT PRIMARY KEY,
  description VARCHAR(40) UNIQUE
) ENGINE = INNODB;

###### - 3 Tabla stateorregions.

CREATE TABLE IF NOT EXISTS stateorregions (
  code VARCHAR(6) PRIMARY KEY,
  name VARCHAR(60) ,
  country_id VARCHAR(6),
  code3166 VARCHAR(10) UNIQUE,
  subdivision_id INT(11),
  FOREIGN KEY (country_id) REFERENCES countries(isocode),
  FOREIGN KEY (subdivision_id) REFERENCES subdivisioncategories(id)
) ENGINE = INNODB;

###### - 4 Tabla citiesormunicipalities.

CREATE TABLE IF NOT EXISTS citiesormunicipalities ( 
  code VARCHAR(6) PRIMARY KEY,
  name VARCHAR(60) , 
  statereg_id VARCHAR(6),
  FOREIGN KEY (statereg_id ) REFERENCES stateorregions(code)
) ENGINE = INNODB;

###### - 5 Tabla typesidentifications.

CREATE TABLE IF NOT EXISTS typesidentifications (
  id INT(11) PRIMARY KEY,
  description VARCHAR(60) UNIQUE,
  sufix VARCHAR(5)
) ENGINE = INNODB;

###### - 6 Tabla categories.

CREATE TABLE IF NOT EXISTS categories (
  id INT(11) PRIMARY KEY,
  description VARCHAR(60) UNIQUE
) ENGINE = INNODB;

###### - 7 Tabla memberships.

CREATE TABLE IF NOT EXISTS memberships (
  id INT(11) PRIMARY KEY,
  name VARCHAR(50),
  description TEXT
) ENGINE = INNODB;

###### - 8 Tabla periods.

CREATE TABLE IF NOT EXISTS periods (
  id INT(11) PRIMARY KEY,
  name VARCHAR(50) UNIQUE
) ENGINE = INNODB;

###### - 9 Tabla benefits.

CREATE TABLE IF NOT EXISTS benefits (
  id INT(11) PRIMARY KEY,
  description VARCHAR(80),
  details TEXT
) ENGINE = INNODB;

###### - 10 Tabla unitofmeasure.

CREATE TABLE IF NOT EXISTS unitofmeasure (
  id INT(11) PRIMARY KEY,
  description VARCHAR(60) UNIQUE
) ENGINE = INNODB;

###### - 11 Tabla categories_polls.

CREATE TABLE IF NOT EXISTS categories_polls (
  id INT(11) PRIMARY KEY,
  name VARCHAR(80) UNIQUE
) ENGINE = INNODB;

###### - 12 Tabla polls.

CREATE TABLE IF NOT EXISTS polls (
  id INT(11) PRIMARY KEY,
  name VARCHAR(80) UNIQUE,
  description TEXT,
  isactive BOOLEAN,
  categorypoll_id INT(11),
  FOREIGN KEY (categorypoll_id) REFERENCES categories_polls(id)
) ENGINE = INNODB;

###### - 13 Tabla products.

CREATE TABLE IF NOT EXISTS products (
  id INT(11) PRIMARY KEY,
  name VARCHAR(60) UNIQUE,
  detail TEXT,
  price DOUBLE,
  category_id INT(11), 
  image VARCHAR(80),
  FOREIGN KEY (category_id) REFERENCES categories(id)
) ENGINE = INNODB;

###### - 14 Tabla audiences.

CREATE TABLE IF NOT EXISTS audiences (
  id INT(11) PRIMARY KEY,
  description VARCHAR(60)
) ENGINE = INNODB;

###### - 15 Tabla customers.

CREATE TABLE IF NOT EXISTS customers (
  id INT(11) PRIMARY KEY,
  name VARCHAR(80),
  city_id VARCHAR(6),
  audience_id INT(11),
  cellphone VARCHAR(20) UNIQUE,
  email VARCHAR(100) UNIQUE,
  address VARCHAR(120),
  FOREIGN KEY (city_id) REFERENCES citiesormunicipalities(code),
  FOREIGN KEY (audience_id) REFERENCES audiences(id)
) ENGINE = INNODB;

###### - 16 Tabla companies.

CREATE TABLE IF NOT EXISTS companies (
  id VARCHAR(20) PRIMARY KEY,
  type_id INT(11), 
  name VARCHAR(80),
  category_id INT(11),
  city_id VARCHAR(6),
  audience_id INT(11),
  cellphone VARCHAR(15) UNIQUE,
  email VARCHAR(80) UNIQUE,
  FOREIGN KEY (type_id) REFERENCES typesidentifications(id),
  FOREIGN KEY (category_id) REFERENCES categories(id),
  FOREIGN KEY (city_id) REFERENCES citiesormunicipalities(code),
  FOREIGN KEY (audience_id) REFERENCES audiences(id)
) ENGINE = INNODB;

###### - 17 Tabla audiencebenefits.

CREATE TABLE IF NOT EXISTS audiencebenefits(
  audience_id INT(11),
  benefit_id INT(11),
  PRIMARY KEY (audience_id, benefit_id),
  FOREIGN KEY (audience_id) REFERENCES audiences(id),
  FOREIGN KEY (benefit_id) REFERENCES benefits(id)
) ENGINE = INNODB;

###### - 18 Tabla membershipperiods.

CREATE TABLE membershipperiods (
  membership_id INT(11),
  period_id INT(11),
  price DOUBLE(10,2),
  PRIMARY KEY (membership_id, period_id),
  FOREIGN KEY (membership_id) REFERENCES memberships(id),
  FOREIGN KEY (period_id) REFERENCES periods(id)
) ENGINE = INNODB;

###### - 19 Tabla favorites.

CREATE TABLE IF NOT EXISTS favorites (
  id INT(11) PRIMARY KEY,
  customer_id INT(11),
  company_id VARCHAR(20),
  FOREIGN KEY (customer_id) REFERENCES customers(id),
  FOREIGN KEY (company_id) REFERENCES companies(id)
) ENGINE = INNODB;

###### - 20 Tabla companyproducts.

CREATE TABLE companyproducts (
  company_id VARCHAR(20),
  product_id INT(11),
  price DOUBLE(10,2),
  unitmeasure_id INT(11),
  PRIMARY KEY (company_id, product_id),
  FOREIGN KEY (company_id) REFERENCES companies(id),
  FOREIGN KEY (product_id) REFERENCES products(id),
  FOREIGN KEY (unitmeasure_id) REFERENCES unitofmeasure(id)
) ENGINE = INNODB;

###### - 21 Tabla detalis_favorites.

CREATE TABLE IF NOT EXISTS details_favorites(
  id INT(11) PRIMARY KEY,
  favorite_id INT(11),
  product_id INT(11),
  FOREIGN KEY (favorite_id) REFERENCES favorites(id),
  FOREIGN KEY (product_id) REFERENCES products(id)
) ENGINE = INNODB;

###### - 22 Tabla rates.

CREATE TABLE rates (
  customer_id INT(11),
  company_id VARCHAR(20),
  poll_id INT(11),
  daterating DATETIME,
  rating DOUBLE(10,2),
  PRIMARY KEY (customer_id, company_id, poll_id),
  FOREIGN KEY (customer_id) REFERENCES customers(id),
  FOREIGN KEY (company_id) REFERENCES companies(id),
  FOREIGN KEY (poll_id) REFERENCES polls(id)
);

###### - 23 Tabla quality_products.

CREATE TABLE quality_products (
  product_id INT(11),
  customer_id INT(11),
  poll_id INT(11),
  company_id VARCHAR(20),
  daterating DATETIME,
  rating DOUBLE(10,2),
  PRIMARY KEY (product_id, customer_id, poll_id, company_id),
  FOREIGN KEY (product_id) REFERENCES products(id),
  FOREIGN KEY (customer_id) REFERENCES customers(id),
  FOREIGN KEY (poll_id) REFERENCES polls(id),
  FOREIGN KEY (company_id) REFERENCES companies(id)
) ENGINE = INNODB;

###### - 24 Tabla membershipbenefits.

CREATE TABLE membershipbenefits (
  membership_id INT(11),
  period_id INT(11),
  audience_id INT(11),
  benefit_id INT(11),
  PRIMARY KEY (membership_id, period_id, audience_id, benefit_id),
  FOREIGN KEY (membership_id) REFERENCES memberships(id),
  FOREIGN KEY (period_id) REFERENCES periods(id),
  FOREIGN KEY (audience_id) REFERENCES audiences(id),
  FOREIGN KEY (benefit_id) REFERENCES benefits(id)
) ENGINE = INNODB; 

#### - Tablas adicionales

###### - 25 Tabla 



